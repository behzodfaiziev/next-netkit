{
  "name": "next-netkit",
  "version": "0.3.3",
  "description": "Network manager",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "test": "jest",
    "testPublish": "rm -rf dist && npm install && npm run build && npm test && npm publish",
    "testPublish:dev": "rm -rf dist && npm install && npm run build && npm publish --tag dev"
  },
  "keywords": [],
  "author": "Behzodjon Faiziev",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/behzodfaiziev/next-netkit.git"
  },
  "bugs": {
    "url": "https://github.com/behzodfaiziev/next-netkit/issues"
  },
  "homepage": "https://github.com/behzodfaiziev/next-netkit#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  }
}
